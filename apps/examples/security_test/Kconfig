config EXAMPLES_SECURITY_API_TEST
	bool "Security API Test program"
	default n
	depends on SECURITY_API && SECURITY_LINK && CONFIG_SE 
	select STRESS_TOOL
	---help---
		Enable Security API Test

config USER_ENTRYPOINT
	string
	default "security API test" if ENTRY_SECURITY_API_TEST
